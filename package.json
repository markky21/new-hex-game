{
  "name": "new-hex-game",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "pretty": "pretty-quick --pattern \"src/**/*.(ts | tsx | js)\""
  },
  "dependencies": {
    "@datorama/akita": "^5.1.1",
    "@nestjs/core": "^7.2.0",
    "@react-spring/three": "^9.0.0-rc.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "jest": "24.9.0",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "react-three-fiber": "^4.2.10",
    "react-use": "^15.3.2",
    "rxjs": "^6.5.5",
    "socket.io-client": "^2.3.0",
    "stats.js": "^0.17.0",
    "three": "^0.117.1",
    "typescript": "~3.7.2"
  },
  "devDependencies": {
    "@datorama/akita-ngdevtools": "^5.0.3",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/socket.io-client": "^1.4.33",
    "@types/stats.js": "^0.17.0",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pretty",
      "pre-push": "npm run pretty"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "jsxBracketSameLine": true,
    "tabWidth": 2,
    "printWidth": 120
  },
  "akitaCli": {
    "basePath": "./src/store/",
    "template": "ts",
    "idKey": "id",
    "customFolderName": true
  }
}
